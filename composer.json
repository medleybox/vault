{
    "name": "medleybox/vault",
    "description": "A Symfony 5 application to retrieve and store music for Medlybox.",
    "type": "project",
    "license": "proprietary",
    "config": {
        "platform": {
            "php": "8.1"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "require": {
        "php": "^8.1",
        "ext-amqp": "*",
        "composer-plugin-api": "^2",
        "amphp/websocket-client": "^1.0",
        "cboden/ratchet": "^0.4.3",
        "doctrine/annotations": "^1.0",
        "doctrine/collections": "^1.6",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/orm": "^2.9.2",
        "dunglas/doctrine-json-odm": "^1.0",
        "giggsey/psr7-stream-response": "^0.1.3",
        "guzzlehttp/psr7": "^1.7",
        "james-heinrich/getid3": "^1.9",
        "league/csv": "^9.6",
        "league/flysystem-aws-s3-v3": "^1.0.29",
        "madcoda/php-youtube-api": "^1.2",
        "phpdocumentor/reflection-docblock": "^5.2",
        "ramsey/uuid": "^4.0",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/console": "^5",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^5",
        "symfony/http-client": "^5",
        "symfony/messenger": "^5",
        "symfony/monolog-bundle": "^3.5",
        "symfony/process": "^5",
        "symfony/property-access": "^5",
        "symfony/property-info": "^5",
        "symfony/proxy-manager-bridge": "^5",
        "symfony/runtime": "^5",
        "symfony/serializer": "^5",
        "symfony/yaml": "^5"
    },
    "require-dev": {
        "phpstan/phpstan": "~1.4.9",
        "phpstan/phpstan-doctrine": "^1.2",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/debug-bundle": "^5",
        "symfony/maker-bundle": "^1.14"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "MedleyBox\\Plugins\\": "src/Plugins/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*",
        "doctrine/doctrine-migrations-bundle": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "~5.4"
        }
    }
}
